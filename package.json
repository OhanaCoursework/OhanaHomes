  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "unit-tests": "mocha src/tests/unitTests",
    "linter": "npx eslint .",
    "ui-tests": "jest test __test__/seleniumTests",
